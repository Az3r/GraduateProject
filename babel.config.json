{
  "presets": ["next/babel"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ".",
        "alias": {
          "@components": ["./components"],
          "@libs": ["./libs"],
          "@models": ["./model"],
          "@styles": ["./styles"],
          "@utils": ["./utils"],
          "@server": ["./libs/server"],
          "@client": ["./libs/client"],
          "@hooks": ["./hooks"]
        }
      }
    ]
  ]
}
